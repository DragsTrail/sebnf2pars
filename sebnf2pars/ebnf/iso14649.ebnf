	;

(* two5DmillingStrategy also has subtypes bidirectionalContour,         *)
(* contourBidirectional, contourSpiral, centerMilling, explicitStrategy *)
two5DmillingStrategy =
	  contourParallel
	| bidirectionalMilling
	| unidirectionalMilling
	;

unidirectionalMilling = 
	  UNIDIRECTIONALMILLING , '(' , optReal , c , optBoolean , c ,
	  optDirection , c , optCutmodeType , ')'
	;

unloadTool =
	  UNLOADTOOL , '(' , CharString , c , optMachiningTool , ')'
	;

unspecified =
	  UNSPECIFIED
	;

uvStrategy =
	  UVSTRATEGY , '(' , pathmodeType , c , cutmodeType , c ,
          tolerances , c , optReal , c , direction , c , direction , ')'
	;

vector =
	  VECTOR , '(' , CharString , c , direction , c , RealString , ')'
	;

veeProfile =
	  VEEPROFILE , '(' , optAxis2placement3d , c ,
          tolerancedLengthMeasure , c , RealString , c , RealString , ')'
	;

waitForMark =
	  WAITFORMARK , '(' , CharString , c , channel , ')'
	;

woodruffSlotEndType =
	  WOODRUFFSLOTENDTYPE, '(' , tolerancedLengthMeasure , ')'
	;

workingstep =
	  machiningWorkingstep
	| rapidMovement
	| touchProbing
	;

workpiece =
	  WORKPIECE , '(' , CharString , c , optMaterial , c , optReal , c ,
	  optWorkpiece , c , optAdvancedBrepShapeRepresentation , c ,
	  optBoundingGeometrySelect , c , parenCartesianPointList , ')'
	;

workpieceCompleteProbing =
	  WORKPIECECOMPLETEPROBING , '(' , CharString , c ,
          elementarySurface , c , optToolpathList , c , optToolDirection , c ,
          ncVariable , c , workpiece , c , tolerancedLengthMeasure , c ,
          touchProbe , c , offsetVector , ')'
	;

workpieceList =
	  workpiece
	| workpieceList , c , workpiece
	;

workpieceProbing =
	  WORKPIECEPROBING , '(' , CharString , c , elementarySurface , c ,
	  optToolpathList , c , optToolDirection , c , ncVariable , c ,
          axis2placement3d , c , workpiece , c , direction , c ,
          tolerancedLengthMeasure , c , touchProbe , ')'
	;

workpieceSetup =
	  WORKPIECESETUP , '(' , workpiece , c , axis2placement3d , c ,
	  optOffsetVector , c , optRestrictedAreaSelect , c , 
	  parenSetupInstructionList , ')'
	;

workpieceSetupList =
	  workpieceSetup
	| workpieceSetupList , c , workpieceSetup
	;

workplan =
	  WORKPLAN , '(' , CharString , c , parenExecutableList , c ,
	  optChannel , c , optSetup , c , optInProcessGeometry , ')'
	;

