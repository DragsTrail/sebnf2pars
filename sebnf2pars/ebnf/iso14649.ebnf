(*=============================================================*)

(* This EBNF file represents STEP Part 21 files based on       *)
(* Parts 10 and 11 of ISO 14649. About three-quarters of Parts *)
(* 10 and 11 is covered.                                       *)

(* The following things in Parts 10 and 11 of ISO 14649 have   *)
(* been omitted:                                               *)
(* booleanExpression                                           *)
(* programStructures other than workplan                       *)
(* splines of all sorts                                        *)
(* surfaceTextureParameter                                     *)
(* toolDirectionForMilling                                     *)
(* topology of all sorts                                       *)

(* The following things in Parts 10 and 11 of ISO 14649 have   *)
(* been abbreviated:                                           *)
(* advancedBrepShapeRepresentation                             *)
(* approachRetractStrategy                                     *)
(* date                                                        *)
(* drill                                                       *)
(* drillingTypeOperation                                       *)
(* machiningFeature                                            *)
(* millingCutter                                               *)
(* millingToolBody                                             *)
(* personAndAddress                                            *)
(* region                                                      *)
(* representations                                             *)
(* toolpath                                                    *)
(* trajectory                                                  *)
(* two5DmillingStrategy                                        *)

(*=============================================================*)
(*                                                             *)
(* The language used in this file to describe Part 21 input    *)
(* conforms to the ISO 14977 standard for EBNF (Extended       *)
(* Backus-Naur Form), with the following conventions.          *)
(*                                                             *)
(* 1. Words spelled here with all upper case letters are       *)
(*    keywords. The actual spelling of these words in an input *)
(*    file may use upper or lower case letters. If a keyword   *)
(*    is not spelled as it appears, the spelling is given      *)
(*    here. Otherwise, its spelling is not given.              *)
(*                                                             *)
(* 2. Words spelled here starting with an upper case letter    *)
(*    and containing lower case letters may be defined in      *)
(*    terms of characters alone but are not defined here.      *)
(*    They are listed separately below with dummy definitions. *)
(*                                                             *)
(* 3. Words spelled here starting with a lower case letter     *)
(*    are non-terminal symbols. Every one of them appears      *)
(*    exactly once as the symbol being defined (i.e. the       *)
(*    symbol on the left side of the equal sign).              *)
(*                                                             *)
(* Meanings of the standard EBNF symbols used here are:        *)
(*   |        ... alternative expression                       *)
(*   (  )     ... group (the usual meaning of parentheses)     *)
(*   '  '     ... literal text (one or more characters)        *)
(*   =        ... assignment                                   *)
(*   ,        ... separates terms not otherwise separated      *)
(*   ;        ... end of statement                             *)
(*                                                             *)
(* Since EBNF and part 21 both use commas profusely, the Part  *)
(* 21 comma is defined here as "c" rather than shown as ',' .  *)
(*                                                             *)
(*=============================================================*)

(*=============================================================*)
(* starting symbol                                             *)
(*=============================================================*)

inputFile =
	  fileStart , headerSection , dataSection , fileEnd
	;

(*=============================================================*)
(* comma *)
(*=============================================================*)

c = ','
	;

(*=============================================================*)
(* nonterminals for all Part 21 files                          *)
(*=============================================================*)

dataSection =
	  dataStarter , instanceList , endSection
	;

dataStarter =
	  DATA , ';'
	;

endSection =
	  ENDSEC , ';'
	;

fileDescription =
	  FILEDESCRIPTION , '(' , parenStringListFull , c ,
          CharString , ')' , ';'
	;

FILEDESCRIPTION =
	  'F','I','L','E','_','D','E','S','C','R','I','P','T','I','O','N'
	;

fileEnd =
	  FILEEND , ';'
	;

FILEEND =
	  'E','N','D','-','I','S','O','-','1','0','3','0','3','-','2','1'
	;

fileName =
	  FILENAME , '(' , CharString , c , CharString , c ,
	  parenStringListFull , c , parenStringListFull , c ,
	  CharString , c , CharString , c , CharString , ')' , ';'
	;

FILENAME =
	  'F','I','L','E','_','N','A','M','E'
	;

fileSchema = FILESCHEMA , '(' , parenStringList , ')' , ';'
	;

FILESCHEMA =
	  'F','I','L','E','_','S','C','H','E','M','A'
	;

fileStart =
	  FILESTART , ';'
	;

FILESTART =
	  'I','S','O','-','1','0','3','0','3','-','2','1'
	;

headerSection =
	  headerStarter , fileDescription , fileName , fileSchema , endSection
	;

headerStarter =
	  HEADER , ';'
	;

instanceId =
	  '#' , IntString
	;

instanceList =
	  instancePlus
	| instanceList , instancePlus
	;

instancePlus =
	  instanceId , '=' , instance , ';'
	;

optReal =
	  real
	| '$'
	;

parenRealListFull =
	  '(' , realList , ')'
	;

parenStringList =
	  '(' , stringList , ')'
	| '(' , ')'
	;

parenStringListFull =
	  '(' , stringList , ')'
	;

real =
	  RealString
	;

realList =
	  real
	| realList , c , real
	;

stringList =
	  CharString
	| stringList , c , CharString
	;

